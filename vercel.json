{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1",
      "check": true
    },
    {
      "src": "/",
      "dest": "index.js"
    }
  ]
}
